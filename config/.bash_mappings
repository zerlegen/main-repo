# COMMANDS

## system information 

alias cpu-arch-show--cas=$BIN_ARCH
alias cas=cpu-arch-show--cas

alias date-show--das=$BIN_DATE
alias das=date-show--das

alias date-set--dase_MMddhhmmYYYY=$BIN_DATE 
alias dase=date-set--dase_MMddhhmmYYYY

alias host-id-show--hids=$BIN_HOSTID
alias hids=host-id-show--hids

alias host-name-show--hns=$BIN_HOSTNAME
alias hns=host-name-show--hns

alias ipc-status-show--iss=$BIN_IPCS
alias iss=ipc-status-show--iss

alias user-default-locale-show--udls_user="$BIN_LOCALE -u"
alias udls=user-default-locale-show--udls_user

alias system-default-locale-show--sdls="$BIN_LOCALE -s"
alias sdls=system-default-locale-show--sdls

alias env-variables-show--evs="$BIN_PRINTENV"
alias evs=env-variables-show--evs

alias platform-info-show--pis="$BIN_UNAME -a"
alias pis=platform-info-show--pis



## devices

#copy-device-data_src_dest_blocksize_blockcount() {
device-data-copy--ddc_src_dest_blocksize_blockcount() {
  $BIN_DD if=$1 of=$2 bs=$3 count=$4
}
alias ddc=device-data-copy--ddc_src_dest_blocksize_blockcount


## filesystem 
alias file-move--fm=$BIN_MV
alias fm=file-move--fm

alias file-rename--fr=$BIN_MV
alias fr=file-rename--fr

alias file-copy--fcop_src_dest=$BIN_CP
alias fcop=file-copy--fcop_src_dest

alias files-show-all--fsa="$BIN_LS"
alias fsa=files-show-all--fsa

alias file-show-meta--fsm_file="$BIN_LS -alh"
alias fsm=file-show-meta--fsm_file

alias files-show-all-meta--fsam="$BIN_LS -alh"
alias fsam=files-show-all-meta--fsam

alias file-delete=$BIN_RM
alias fd=file-delete

alias file-delete-FORCE="$BIN_RM -f"
alias fdF=file-delete-FORCE

