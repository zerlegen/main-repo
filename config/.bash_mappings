## processes
signal-send--sigs_signal_process() {
  $BIN_KILL -$1 $2
}
alias sigsn=signal-send--sigs_signal_process

alias signals-show--sigsh="$BIN_KILL -l"
alias sigsh=signals-show--sigsh

process-change-priority--pscp_priority_process-id() {
  $BIN_RENICE -n $1 $2 
}
alias pscp=process-change-priority--pscp_priority_process-id

alias process-run-with-timeout--psrwt_timeout-sec_command=$BIN_TIMEOUT
alias psrwt=process-run-with-timeout--psrwt



## security

sec-get-random-data--scgrd_dest_blocksize_blockcount() {
  $BIN_DD if=/dev/random of=$1 bs=$2 count=$3
}
alias scgrd=sec-get-random-data--scgrd_dest_blocksize_blockcount


sec-get-pseudo-random-data--scgprd_dest_blocksize_blockcount() {
  $BIN_DD if=/dev/urandom of=$1 bs=$2 count=$3
}
alias scgprd=sec-get-pseudo-random-data--scgprd_dest_blocksize_blockcount

## user accounts

alias user-get-groups--ugg=$BIN_GROUPS
alias ugg=user-get-groups--ugg


user-get-current-ids--ugci_user() {
  $BIN_ID $1
}
alias ugci=user-get-current-ids--ugci_user

selinux-get-security-context-of-user--selgscou_user() {
  $BIN_ID -Z $1
}
alias selgscou=selinux-get-security-context-of-user--selgscou_user

alias user-default-locale-show--udls_user="$BIN_LOCALE -u"
alias udls=user-default-locale-show--udls_user


