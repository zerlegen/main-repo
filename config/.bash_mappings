#alias show-platform-info="$BIN_UNAME -a"
alias platform-info-show="$BIN_UNAME -a"



## devices

#copy-device-data_src_dest_blocksize_blockcount() {
device-data-copy_src_dest_blocksize_blockcount() {
  $BIN_DD if=$1 of=$2 bs=$3 count=$4
}


## filesystem 
alias file-move=$BIN_MV
alias fm=file-move

alias file-rename=$BIN_MV
alias fr=file-rename

alias file-copy_src_dest=$BIN_CP
alias fcop_src_dest=file-copy_src_dest

alias files-show-all="$BIN_LS -alh"
alias fsa=files-show-all

alias file-delete=$BIN_RM
alias fd=file-delete

alias file-delete-FORCE="$BIN_RM -f"
alias fdF=file-delete-FORCE

alias file-status-show=$BIN_STAT
alias fss=file-status-show

alias file-change-permissions_bitmask_file=$BIN_CHMOD
alias fcp_bitmask_file=file-change-permissions_bitmask_file

alias file-change-owner_username_file=$BIN_CHOWN
alias fcow_username_file=file-change-owner_username_file

alias file-change-group_groupname_file=$BIN_CHGRP
alias fcg_groupname_file=file-change-group_groupname_file

alias filesystem-status-show=$BIN_STAT
alias fsss=filesystem-status-show

alias file-basename-show=basename
alias fbs=file-basename-show

alias file-permissions-show="$BIN_GETFACL -a"
alias fps=file-permissions-show

alias file-byte-count-show="$BIN_WC -c"
alias fbcs=file-byte-count-show

alias file-word-count-show="$BIN_WC -w"
alias fwcs=file-word-count-show

alias file-line-count-show="$BIN_WC -l"
alias flcs=file-line-count-show

alias file-char-count-show="$BIN_WC -m"
alias fccs=file-char-count-show

alias file-reverse-show=$BIN_TAC
alias frs=file-reverse-show

alias file-printable-strings-show=$BIN_STRINGS
alias fpss=file-printable-strings-show

alias file-end-show=$BIN_TAIL
alias fes=file-end-show

alias file-watch-end=$BIN_TAILF
alias fwe=file-watch-end

alias filesystem-sync-buffers=$BIN_SYNC
alias fssb=filesystem-sync-buffers

alias dirs-show="$BIN_LS -ld"
alias ds=dirs-show

alias dir-make_path="$BIN_MKDIR"
alias dm_path=dir-make_path

alias dir-current-show=$BIN_PWD
alias dcs=dir-current-show

alias dir-delete-empty=$BIN_RMDIR
alias dde=dir-delete-empty

alias dirtree-copy_src_dest="$BIN_CP -R"
alias dtc_src_dest=dirtree-copy_src_dest

alias dirtree-delete="$BIN_RM -r"
alias dtd=dirtree-delete

alias dirtree-delete-FORCE="$BIN_RM -rf"
alias dtdF=dirtree-delete-FORCE

alias selinux-change-security-context-of-file=$BIN_CHCON
alias selcsc=selinux-change-security-context-of-file

alias symlink-create_target-file_link-name="$BIN_LN -s"
alias slc_target-file_link-name=symlink-create_target-file_link-name

alias symlink-view=$BIN_READLINK
alias slv=symlink-view

alias disk-space-show-free="$BIN_DF -h"
alias dssf=disk-space-show-free

alias disk-space-current-dir-show="$BIN_DU -h"
alias dscds=disk-space-current-dir-show


### compression/encoding/decoding
alias file-compress-bzip2_infile=$BIN_BZIP2
alias file-uncompress-bzip2_infile="$BIN_BZIP2 -d"
alias file-uncompress-and-show-bzip2_infile="$BIN_BZIP2 -c -d"
file-compress-gzip_infile_outfile() {
  $BIN_GZIP -c $1 > $2
}
alias file-uncompress-gzip_infile="$BIN_GZIP -df"

alias file-hash-crc-show=$BIN_CKSUM
alias file-hash-crc32-show=$BIN_CRC32
alias file-hash-md5-show=$BIN_MD5SUM
alias file-hash-sha1-show=$BIN_SHA1SUM
alias file-hash-sha256-show=$BIN_SHA256SUM
alias file-hash-sha384-show=$BIN_SHA384SUM
alias file-hash-sha512-show=$BIN_SHA512SUM
alias file-encode-to-base64=$BIN_BASE64
alias file-decode-from-base64="$BIN_BASE64 -d"

alias file-convert-line-endings-to-dos=$BIN_UNIX2DOS
alias file-convert-line-endings-to-unix=$BIN_DOS2UNIX
alias file-convert-tabs-to-spaces_infile_outfile=$BIN_EXPAND
alias file-convert-spaces-to-tabs_infile_outfile=$BIN_UNEXPAND

alias file-get-type_infile=$BIN_FILE

file-view-hex_offset_file() {
  $BIN_HEXDUMP -C -s $1 $2 | less
}

alias char-encodings-show="$BIN_ICONV -l"
char-encoding-convert_incoding_outcoding_infile_outfile() {
  $BIN_ICONV -f $1 -t $2 $3 > $4 
}



## network 
alias start-ftp=$BIN_FTP
copy-tree-secure_local-str_remote-str() {
  $BIN_SCP -r $1 $2
}
alias download="$BIN_WGET"

## math
alias factor-integer_int=$BIN_FACTOR

## programming 
alias c-complier=$BIN_GCC
alias awk=$BIN_AWK
alias gnu-assembler=$BIN_AS
alias cpp-compiler="$BIN_GPP"
alias gnu-debugger=$BIN_GDB
alias gnu-linker=$BIN_LD
alias show-lib-dependencies_exe-or-lib=$BIN_LDD
alias gnu-make=$BIN_MAKE
alias show-binary-section-sizes_binary=$BIN_SIZE

## processes
send-signal_signal_process() {
  $BIN_KILL -$1 $2
}
alias show-signals="$BIN_KILL -l"
change-process-priority_priority_process-id() {
  $BIN_RENICE -n $1 $2 
}


## security

get-random-data_dest_blocksize_blockcount() {
  $BIN_DD if=/dev/random of=$1 bs=$2 count=$3
}

get-pseudo-random-data_dest_blocksize_blockcount() {
  $BIN_DD if=/dev/urandom of=$1 bs=$2 count=$3
}

## user accounts

alias get-groups-for-user_user=$BIN_GROUPS


get-current-user-ids_user() {
  $BIN_ID $1
}

get-selinux-security-context-of-user_user() {
  $BIN_ID -Z $1
}

## misc commands

alias show-calendar=$BIN_CAL
alias view-file-by-page=$BIN_LESS
alias view-file-elf="$BIN_READELF -a"
alias sort-file-lines_file="$BIN_SORT"
alias run-with-timeout=$BIN_TIMEOUT
alias generate-uuid=$BIN_UUIDGEN
alias print-until-killed_string=$BIN_YES
