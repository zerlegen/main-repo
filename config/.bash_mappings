# COMMANDS

## system information 

alias system-cpu-arch-show--scas=$BIN_ARCH
alias scas=cpu-arch-show--scas

alias system-date-show--sdas=$BIN_DATE
alias sdas=system-date-show--sdas

alias system-date-set--sdase_MMddhhmmYYYY=$BIN_DATE 
alias sdase=system-date-set--sdase_MMddhhmmYYYY

alias system-host-id-show--shids=$BIN_HOSTID
alias shids=system-host-id-show--shids

alias system-host-name-show--shns=$BIN_HOSTNAME
alias shns=system-host-name-show--shns

alias system-ipc-status-show--siss=$BIN_IPCS
alias siss=system-ipc-status-show--siss

alias system-default-locale-show--sdls="$BIN_LOCALE -s"
alias sdls=system-default-locale-show--sdls

alias system-env-variables-show--sevs="$BIN_PRINTENV"
alias sevs=system-env-variables-show--sevs

alias system-platform-info-show--spis="$BIN_UNAME -a"
alias spis=system-platform-info-show--spis

alias system-os-show--soss="echo $OSTYPE"
alias soss=system-os-show--soss



## devices

#copy-device-data_src_dest_blocksize_blockcount() {
device-data-copy--ddc_src_dest_blocksize_blockcount() {
  $BIN_DD if=$1 of=$2 bs=$3 count=$4
}
alias ddc=device-data-copy--ddc_src_dest_blocksize_blockcount


## filesystem 
alias file-move--fm=$BIN_MV
alias fm=file-move--fm

alias file-rename--fr=$BIN_MV
alias fr=file-rename--fr

alias file-copy--fcop_src_dest=$BIN_CP
alias fcop=file-copy--fcop_src_dest

alias files-show-all--fsa="$BIN_LS"
alias fsa=files-show-all--fsa

alias file-show-meta--fsm_file="$BIN_LS -alh"
alias fsm=file-show-meta--fsm_file

alias files-show-all-meta--fsam="$BIN_LS -alh"
alias fsam=files-show-all-meta--fsam

alias file-delete=$BIN_RM
alias fd=file-delete

alias file-delete-FORCE="$BIN_RM -f"
alias fdF=file-delete-FORCE

alias file-status-show=$BIN_STAT
alias fss=file-status-show

alias file-change-permissions--fcp_bitmask_file=$BIN_CHMOD
alias fcp=file-change-permissions--fcp_bitmask_file

alias file-change-owner--fchown_username_file=$BIN_CHOWN
alias fchown=file-change-owner--fchown_username_file

alias file-change-group--fcgr_groupname_file=$BIN_CHGRP
alias fcgr=file-change-group--fcgr_groupname_file

alias filesystem-status-show--fsss_path=$BIN_STAT
alias fsss=filesystem-status-show--fsss_path

alias file-basename-show--fbs=basename
alias fbs=file-basename-show--fbs

alias file-permissions-show--fps="$BIN_GETFACL -a"
alias fps=file-permissions-show--fps

alias file-byte-count-show--fbcs="$BIN_WC -c"
alias fbcs=file-byte-count-show--fbcs

alias file-word-count-show--fwcs="$BIN_WC -w"
alias fwcs=file-word-count-show--fwcs

alias file-line-count-show--flcs="$BIN_WC -l"
alias flcs=file-line-count-show--flcs

alias file-char-count-show--fccs="$BIN_WC -m"
alias fccs=file-char-count-show--fccs

alias file-reverse-lines-show--frls=$BIN_TAC
alias frls=file-reverse-lines-show--frls

