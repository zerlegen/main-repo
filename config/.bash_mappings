#alias set-date_MMddhhmmYYYY=$BIN_DATE 
alias date-set_MMddhhmmYYYY=$BIN_DATE 


alias host-id-show=$BIN_HOSTID
alias host-name-show=$BIN_HOSTNAME


#alias get-ipc-status=$BIN_IPCS
alias ipc-status-show=$BIN_IPCS


#alias get-user-default-locale_user="$BIN_LOCALE -u"
alias user-default-locale-show_user="$BIN_LOCALE -u"


#alias get-system-default-locale="$BIN_LOCALE -s"
alias system-default-locale-show="$BIN_LOCALE -s"

#alias show-environment-variables="$BIN_PRINTENV"
